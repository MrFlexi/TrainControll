<core:FragmentDefinition xmlns:core="sap.ui.core">
	<sap.m:List xmlns:sap.m="sap.m" noDataText="Drop list items here" items="{LokListModel>/}" id="__list0123">
							<sap.m:items>
								<sap.m:ObjectListItem type="Active" title="{LokListModel>name}" id="__item123" icon="{LokListModel>image_url}">
									<sap.m:attributes>
										<sap.m:ObjectAttribute title="ID" text="{LokListModel>id}" id="__attribute123">
										</sap.m:ObjectAttribute>
										<sap.m:ObjectAttribute title="Address" text="{LokListModel>addr}" id="__attribute223">
										</sap.m:ObjectAttribute>
										<sap.m:ObjectAttribute title="Type" text="{LokListModel>type}" id="__attribute323">
										</sap.m:ObjectAttribute>
										<sap.m:ObjectAttribute title="Bound to client" text="{LokListModel>client_id}" id="__attribute4">
										</sap.m:ObjectAttribute>
										<sap.m:ObjectAttribute title="User" text="{LokListModel>user_name}" id="__attribute5">
										</sap.m:ObjectAttribute>
										<sap.m:ObjectAttribute title="Speed" text="{LokListModel>speed}" id="__attribute6">
										</sap.m:ObjectAttribute>
										<sap.m:ObjectAttribute title="Direction" text="{LokListModel>dir}" id="__attribute7">
										</sap.m:ObjectAttribute>
									</sap.m:attributes>
									<sap.m:firstStatus>
										<sap.m:ObjectStatus
															text="{LokListModel>status}"
															state="{	path: 'LokListModel>status',
																		formatter: 'view.Formatter.status'	}" />
									</sap.m:firstStatus>
								</sap.m:ObjectListItem>
							</sap.m:items>
	</sap.m:List>

</core:FragmentDefinition>